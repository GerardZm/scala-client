package controllers

import scalaj.http._
import java.io.File
import play.api._
import play.api.mvc._

class post() extends Controller {
  
  def execute() {
    val request = Http("http://192.168.127.129:3000/code")
      .postForm(Seq("code" -> "abc"))
      //.timeout(connTimeoutMs = 1000, readTimeoutMs = 5000)
      .asString
    println(request.body);
  }
  
}
